				<xs:sequence>
					<xs:element name="messageBody" type="fundMasterDataBodyType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="fundMasterDataBodyType">
		<xs:sequence>
			<xs:element name="fundMasterDataList" type="fundMasterDataListType"/>
		</xs:sequence>
	</xs:complexType>
	
