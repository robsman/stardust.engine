						<xs:restriction base="xs:string">
							<xs:maxLength value="20" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="votingStock">
					<xs:annotation>
						<xs:documentation>Issuer number of stock eligible for voting</xs:documentation>
						<xs:documentation>Donnees_complementaires_objet.Valeur_zone (VOTISS)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="20"/>
							<xs:fractionDigits value="7"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="nonVotingStock">
					<xs:annotation>
						<xs:documentation>Issuer number of stock not eligible for voting</xs:documentation>
						<xs:documentation>Donnees_complementaires_objet.Valeur_zone (NVOTIS)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="20"/>
							<xs:fractionDigits value="7"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="ratingList">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="rating" maxOccurs="unbounded" minOccurs="0">
								<xs:complexType>
									<xs:all>
										<xs:element name="agency">
											<xs:annotation>
												<xs:documentation>Notation agency</xs:documentation>
												<xs:documentation>Liste_tris_par_statut.Type_tri</xs:documentation>
												<xs:documentation>Transcodification RATIAG</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:minLength value="1" />
													<xs:maxLength value="4" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="ratingType">
											<xs:annotation>
												<xs:documentation>Local Short term, Foreign short term, ...</xs:documentation>
												<xs:documentation>Liste_tris_par_statut.Statut_valeur+Statut_ligne</xs:documentation>
												<xs:documentation>Transcodification TYPRAT</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="8" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
										<xs:element name="ratingValue">
											<xs:annotation>
												<xs:documentation>Rating value</xs:documentation>
												<xs:documentation>Liste_tris_par_statut.Critere_tri</xs:documentation>
												<xs:documentation>Transcodification Type_tri</xs:documentation>
											</xs:annotation>
											<xs:simpleType>
												<xs:restriction base="xs:string">
													<xs:maxLength value="8" />
												</xs:restriction>
											</xs:simpleType>
										</xs:element>
									</xs:all>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
		</xs:all>
	</xs:complexType>


