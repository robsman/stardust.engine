		<xs:sequence>
			<xs:element name="fundMasterData" type="fundMasterDataType" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="fundMasterDataType">
		<xs:all>
				<xs:element name="entityType">
					<xs:annotation>
						<xs:documentation>Funds have got a FUND category</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="FUND"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="entityId">
					<xs:annotation>
						<xs:documentation>GP3 fund Id</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.code_portefeuille</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="shortDescription">
					<xs:annotation>
						<xs:documentation>Fund short description</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Libelle_port_c</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="longDescription">
					<xs:annotation>
						<xs:documentation>Fund long description</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Libelle_port_l</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="fundCurrency">
					<xs:annotation>
						<xs:documentation>Fund base currency</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Devise_portefeuille</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mainManager">
					<xs:annotation>
						<xs:documentation>GP3 Fund main manager Id</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Code_gestionnaire</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="investmentPolicy">
					<xs:annotation>
						<xs:documentation>Fund investment policy</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Orientation_gestion</xs:documentation>
						<xs:documentation>Transcodification OPCORI</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="custodian">
					<xs:annotation>
						<xs:documentation>Fund custodian</xs:documentation>
						<xs:documentation>Descriptif_compte_depot.Code_teneur_compte WHERE Descriptif_compte_depot.Code_compte = Descriptif_portefeuille.Code_depositaire_fonds</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="inceptionDate">
					<xs:annotation>
						<xs:documentation>Fund inception Date; YYYY-MM-DD</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Date_creation</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:date">
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="legalType">
					<xs:annotation>
						<xs:documentation>Fund legal type</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Type_fonds</xs:documentation>
						<xs:documentation>Transcodification TYPFON</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="10" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="clientLanguage">
					<xs:annotation>
						<xs:documentation>Fund owner language</xs:documentation>
						<xs:documentation>Descriptif_tiers.Langue_messagerie WHERE Descriptif_tiers.Code_tiers = Descriptif_client.Code_tiers AND Descriptif_client.Code_intervenant = Descriptif_portefeuille.Code_client</xs:documentation>
						<xs:documentation>Transcodification MSGLAN</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="1" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="mainUnitIsinCode">
					<xs:annotation>
						<xs:documentation>Main unit ISIN code</xs:documentation>
						<xs:documentation>Lien_valeur_serveur.Code_valeur_serveur WHERE Lien_valeur_serveur.Code_serveur = 'ISIN' AND Lien_valeur_serveur.Categorie_valeur = 'VMOB' and Lien_valeur_serveur.Code_valeur = Descriptif_part.Code_valeur_majvac</xs:documentation>
						<xs:documentation>AND Descriptif_part.Code_portefeuille = Descriptif_portefeuille.Code_portefeuille and Descriptif_part.Flag_part_ref = 'O'</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="50" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="custodyAccount">
					<xs:annotation>
						<xs:documentation>Fund custody account</xs:documentation>
						<xs:documentation>Descriptif_depot_portefeuille.Numero_compte_depot WHERE Descriptif_depot_portefeuille.Code_portefeuille = Descriptif_portefeuille.Code_portefeuille</xs:documentation>
						<xs:documentation>AND Descriptif_depot_portefeuille.Code_compte = Descriptif_portefeuille.Code_depositaire_fonds</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="34" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="umbrellaCode">
					<xs:annotation>
						<xs:documentation>Umbrella Id, Fund is belonging to</xs:documentation>
						<xs:documentation>Descriptif_portefeuille.Code_sicav</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="masterFundCode">
					<xs:annotation>
						<xs:documentation>Fund master fund Id when available</xs:documentation>
						<xs:documentation>Ensemble_table.Lib_detail_c where Code_table = 'MAITRE' and Code_langue = 'F' and Code_detail = Code_portefeuille</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="parentFundCode">
					<xs:annotation>
						<xs:documentation>GP3 Fund id in case of multi fund manager</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="limitControl">
					<xs:annotation>
						<xs:documentation>compliance limit control</xs:documentation>
						<xs:documentation>Descriptif_port_reporting.regime_ratios</xs:documentation>
						<xs:documentation>Transcodification REGLEG</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="percentageMaxFundShare">
					<xs:annotation>
						<xs:documentation>Maximum percentage of Fund share </xs:documentation>
						<xs:documentation>Donnees_complementaires_objet.Valeur_zone Where Code_zone = 'PCTMAX'</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="8"/>
							<xs:fractionDigits value="5"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
		</xs:all>
	</xs:complexType>


