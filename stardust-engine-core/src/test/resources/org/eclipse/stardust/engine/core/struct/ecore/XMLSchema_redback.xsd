<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema version="1.0" xmlns:xs="http://www.w3.org/2001/XMLSchema">

	<!--  ==========================================================================
									Message headers
	      ==========================================================================   -->

	<xs:complexType name="abstractMessage" abstract="true">
		<xs:complexContent>
			<xs:extension base="abstractMessageItem">
				<xs:sequence>
					<xs:element name="messageHeader" type="MessageHeaderType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name="abstractMessageItem" />

	<xs:complexType name="MessageHeaderType" final="extension restriction">
		<xs:complexContent>
			<xs:extension base="abstractMessageItem">
				<xs:sequence>
					<xs:element name="messageTimeStamp" type="xs:dateTime" />
					<xs:element name="messageType" type="messageType" />
					<xs:element name="senderPk" type="SenderPkType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:simpleType name="messageType">
		<xs:restriction base="xs:string">
			<xs:enumeration value="NOTIFICATION" />
			<xs:enumeration value="ERROR" />
			<xs:enumeration value="INTERNAL" />
			<xs:enumeration value="SERVICE" />
		</xs:restriction>
	</xs:simpleType>

	<xs:complexType name="SenderPkType">
		<xs:complexContent>
			<xs:extension base="abstractMessageItem">
				<xs:sequence>
					<xs:element name="messageId" type="xs:long" />
					<xs:element name="sender" type="xs:string" />
					<xs:element name="serviceUserName" type="xs:string"
						minOccurs="0" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	
	<!-- ==========================================================================
								COMMON TYPES
   		 ========================================================================== -->
	
	<xs:simpleType name="auditTrailType">
		<xs:annotation>
			<xs:documentation>external reference of the transaction; in that case GP3 one</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="16" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="fundCodeType">
		<xs:annotation>
			<xs:documentation>GP3 Fund Id</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="6" />
		</xs:restriction>
	</xs:simpleType>

	<xs:simpleType name="currencyType">
		<xs:annotation>
			<xs:documentation>GP3 currency Id</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1" />
			<xs:maxLength value="3" />
		</xs:restriction>
	</xs:simpleType>

	<!-- ==========================================================================
							CASH TRANSACTION MESSAGE
		 ========================================================================== -->

	<xs:element name="cashTransactionMessage" type="cashTransactionMessageType" />

	<xs:complexType name="cashTransactionMessageType"
		final="extension restriction">
		<xs:complexContent>
			<xs:extension base="abstractMessage">
				<xs:sequence>
					<xs:element name="messageBody" type="cashTransactionBodyType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="cashTransactionBodyType">
		<xs:sequence>
			<xs:element name="cashTransactionList" type="CashTransactionListType"/>
		</xs:sequence>
	</xs:complexType>
	

	<xs:complexType name="CashTransactionListType">
		<xs:sequence>
			<xs:element name="cashTransaction" type="CashTransactionType" maxOccurs="unbounded" minOccurs="0" />
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="CashTransactionType">
		<xs:all>
			<xs:element name="auditTrail" type="auditTrailType"/>
			<xs:element name="fundCode" type="fundCodeType"/>
			<xs:element name="transactionType">
				<xs:annotation>
					<xs:documentation>GP3 transaction type</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:minLength value="1" />
						<xs:maxLength value="4" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="transactionTypeShortDescription">
				<xs:annotation>
					<xs:documentation>GP3 transaction type short description</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="20" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fundCurrency" type="currencyType">
				<xs:annotation>
					<xs:documentation>Currency of the Fund</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="assetCurrency" type="currencyType">
				<xs:annotation>
					<xs:documentation>Currency of the cash movement</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="movementType">
				<xs:annotation>
					<xs:documentation>Indicates if the movement is a cash entry or a cash exit; E for entry; S for exit</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="E" />
						<xs:enumeration value="S" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="tradeDate">
				<xs:annotation>
					<xs:documentation>Indicates the cash movement trade date; YYYY-MM-DD</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:date"></xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="valueDate">
				<xs:annotation>
					<xs:documentation>Indicates the cash movement value date; YYYY-MM-DD</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:date"></xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="sharePar">
				<xs:annotation>
					<xs:documentation>cash movement quantity; not signed</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="18" />
						<xs:fractionDigits value="2" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="fundCostPrice">
				<xs:annotation>
					<xs:documentation>cash movement in Fund currency; not signed</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:decimal">
						<xs:totalDigits value="18" />
						<xs:fractionDigits value="2" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="longDescription">
				<xs:annotation>
					<xs:documentation>GP3 transaction long description</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:maxLength value="50" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:complexType>
	
	<!-- ==========================================================================
							TRANSACTION STATUS MESSAGE
		 ========================================================================== -->
	
	<xs:element name="transactionStatusMessage" type="transactionStatusMessageType" />

	<xs:complexType name="transactionStatusMessageType"
		final="extension restriction">
		<xs:complexContent>
			<xs:extension base="abstractMessage">
				<xs:sequence>
					<xs:element name="messageBody" type="transactionStatusBodyType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="transactionStatusBodyType">
		<xs:sequence>
			<xs:element name="transactionStatusList" type="transactionStatusListType"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="transactionStatusListType">
		<xs:sequence>
			<xs:element name="transactionStatus" type="transactionStatusType" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="transactionStatusType">
		<xs:all>
			<xs:element name="auditTrail" type="auditTrailType"/>
			<xs:element name="status">
				<xs:annotation>
					<xs:documentation>indicates if the trade is Booked; Settled or Deleted; expected values B;S or D</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="PBK" />
						<xs:enumeration value="STD" />
						<xs:enumeration value="PCA" />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:all>
	</xs:complexType>

	<!-- ==========================================================================
							INSTRUMENT MASTER DATA MESSAGE
		 ========================================================================== -->
	
	<xs:element name="instrumentMasterDataMessage" type="instrumentMasterDataMessageType" />

	<xs:complexType name="instrumentMasterDataMessageType"
		final="extension restriction">
		<xs:complexContent>
			<xs:extension base="abstractMessage">
				<xs:sequence>
					<xs:element name="messageBody" type="instrumentMasterDataBodyType" />
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="instrumentMasterDataBodyType">
		<xs:sequence>
			<xs:element name="instrumentMasterDataList" type="instrumentMasterDataListType"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="instrumentMasterDataListType">
		<xs:sequence>
			<xs:element name="instrumentMasterData" type="instrumentMasterDataType" maxOccurs="unbounded" minOccurs="0"/>
		</xs:sequence>
	</xs:complexType>
	
	<xs:complexType name="instrumentMasterDataType">
		<xs:all>
				<xs:element name="entityType">
					<xs:annotation>
						<xs:documentation>GP3 instrument category</xs:documentation>
						<xs:documentation>categorie.valeur</xs:documentation>
						<xs:documentation>TRES is changed into TRET</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="VMOB"/>
							<xs:enumeration value="OPTI"/>
							<xs:enumeration value="FUTU"/>
							<xs:enumeration value="TRET"/>
							<xs:enumeration value="SWAT"/>
							<xs:enumeration value="FCHA"/>
							<xs:enumeration value="FTAU"/>
							<xs:enumeration value="FMAT"/>
							<xs:enumeration value="FIND"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="entityId">
					<xs:annotation>
						<xs:documentation>GP3 instrument Id</xs:documentation>
						<xs:documentation>code_valeur</xs:documentation>
						<xs:documentation>code_compte_treso</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:minLength value="1" />
							<xs:maxLength value="12" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="shortDescription">
					<xs:annotation>
						<xs:documentation>Instrument short description</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.Lib_valeur_c</xs:documentation>
						<xs:documentation>OPTI : Descriptif_option.Lib_option_c</xs:documentation>
						<xs:documentation>FUTU : Descriptif_future.Lib_option_c</xs:documentation>
						<xs:documentation>TRET : Descriptif_compte_tresorerie.Lib_compte_treso</xs:documentation>
						<xs:documentation>SWAT : Descriptif_swap_de_taux.Lib_swap_de_taux_c</xs:documentation>
						<xs:documentation>DIVERS : Descriptif_divers.Lib_valeur_c</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="20" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="longDescription">
					<xs:annotation>
						<xs:documentation>Instrument long description</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.Lib_valeur_l</xs:documentation>
						<xs:documentation>OPTI : Descriptif_option.Lib_option_l</xs:documentation>
						<xs:documentation>FUTU : Descriptif_future.Lib_option_l</xs:documentation>
						<xs:documentation>TRET : Descriptif_compte_tresorerie.Lib_compte_treso</xs:documentation>
						<xs:documentation>SWAT : Descriptif_swap_de_taux.Lib_swap_de_taux_c</xs:documentation>
						<xs:documentation>DIVERS : Descriptif_divers.Lib_valeur_l</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="60" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="issuer">
					<xs:annotation>
						<xs:documentation>GP3 instrument issuer Id</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.Code_emetteur</xs:documentation>
						<xs:documentation>TRET : Parametre_champ_traitement.Valeur_par_defaut (REDBCK,GETMD,TRET ,Issuer)</xs:documentation>
						<xs:documentation>SWAT : Parametre_champ_traitement.Valeur_par_defaut (REDBCK,GETMD,SWAT ,Issuer)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="8" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="issueCurrency">
					<xs:annotation>
						<xs:documentation>Instrument issue currency</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.devise_valeur</xs:documentation>
						<xs:documentation>OPTI : Descriptif_contrat_option.devise_cotation</xs:documentation>
						<xs:documentation>FUTU : Descriptif_contrat_future.devise_cotation</xs:documentation>
						<xs:documentation>TRET : Descriptif_compte_tresorerie.Code_devise</xs:documentation>
						<xs:documentation>DIVERS : Descriptif_divers.Devise_valeur</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="quotationCurrency">
					<xs:annotation>
						<xs:documentation>Instrument quotation currency</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.devise_cotation</xs:documentation>
						<xs:documentation>OPTI : Descriptif_contrat_option.devise_cotation</xs:documentation>
						<xs:documentation>FUTU : Descriptif_contrat_future.devise_cotation</xs:documentation>
						<xs:documentation>TRET : Descriptif_compte_tresorerie.Code_devise</xs:documentation>
						<xs:documentation>DIVERS : Descriptif_divers.Devise_valeur</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="currencyReceive">
					<xs:annotation>
						<xs:documentation>Currency of the receiving leg (SWAT only)Instrument issue currency</xs:documentation>
						<xs:documentation>SWAT : Descriptif_swap_de_taux.Devise_recue</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="currencyPay">
					<xs:annotation>
						<xs:documentation>Currency of the paying leg (SWAT only)Instrument issue currency</xs:documentation>
						<xs:documentation>SWAT : Descriptif_swap_de_taux.Devise_versee</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="3" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="stockExchange">
					<xs:annotation>
						<xs:documentation>GP3 Instrument stock exchange Id</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.place_cotation</xs:documentation>
						<xs:documentation>OPTI : Descriptif_contrat_option.place_cotation</xs:documentation>
						<xs:documentation>FUTU : Descriptif_contrat_future.place_cotation</xs:documentation>
						<xs:documentation>TRET : Parametre_champ_traitement.Valeur_par_defaut (REDBCK,GETMD,TRET ,StockExchange)</xs:documentation>
						<xs:documentation>SWAT : Parametre_champ_traitement.Valeur_par_defaut (REDBCK,GETMD,SWAT ,StockExchange)</xs:documentation>
						<xs:documentation>DIVERS : Descriptif_divers.place_cotation</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="classification">
					<xs:annotation>
						<xs:documentation>GP3 Instrument classification Id</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.classe</xs:documentation>
						<xs:documentation>OPTI : Descriptif_contrat_option.classe</xs:documentation>
						<xs:documentation>FUTU : Donnees_complementaires_objet.valeur_zone WHERE Code_zone = 'CLASSE' and Code_valeur = Identifiant_contrat_future</xs:documentation>
						<xs:documentation>TRET : Parametre_champ_traitement.Valeur_par_defaut (REDBCK,GETMD,TRET ,classification)</xs:documentation>
						<xs:documentation>SWAT : Parametre_champ_traitement.Valeur_par_defaut (REDBCK,GETMD,SWAT ,classification)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="1" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="priceType">
					<xs:annotation>
						<xs:documentation>Instrument price expression</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.expression_cours</xs:documentation>
						<xs:documentation>OPTI : Descriptif_contrat_option.expression_cours</xs:documentation>
						<xs:documentation>FUTU : Descriptif_contrat_option.expression_cours</xs:documentation>
						<xs:documentation>TRET : T</xs:documentation>
						<xs:documentation>SWAT : T</xs:documentation>
						<xs:documentation>DIVERS : P</xs:documentation>
						<xs:documentation>Transcodification TYPCOU</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="H" />
							<xs:enumeration value="%" />
							<xs:enumeration value="T" />
							<xs:enumeration value="P" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="quantityType">
					<xs:annotation>
						<xs:documentation>Instrument quantity expression; Q for quantity; N for nominal; C for contrat</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.expression_quantite</xs:documentation>
						<xs:documentation>OPTI : C</xs:documentation>
						<xs:documentation>FUTU : C</xs:documentation>
						<xs:documentation>TRET : N</xs:documentation>
						<xs:documentation>SWAT : N</xs:documentation>
						<xs:documentation>DIVERS : Q</xs:documentation>
						<xs:documentation>Transcodification EVALUA</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:enumeration value="C" />
							<xs:enumeration value="N" />
							<xs:enumeration value="Q" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="assetType">
					<xs:annotation>
						<xs:documentation>Instrument asset type</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.type_valeur</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="4" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="subCategory">
					<xs:annotation>
						<xs:documentation>VMOB sub category (OBLIG, ACTION, ...)</xs:documentation>
						<xs:documentation>VMOB : Ensemble_table.Code_table_ref_resul (TYPVAL)</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:string">
							<xs:maxLength value="6" />
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="faceValue">
					<xs:annotation>
						<xs:documentation>Instrument face value in issue currency</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.nominal</xs:documentation>
						<xs:documentation>OPTI : 1</xs:documentation>
						<xs:documentation>FUTU : 1</xs:documentation>
						<xs:documentation>TRET : 1</xs:documentation>
						<xs:documentation>DIVERS : Descriptif_divers.Nominal</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="18"/>
							<xs:fractionDigits value="5"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="nominalReceive">
					<xs:annotation>
						<xs:documentation>SWAP nominal leg to receive</xs:documentation>
						<xs:documentation>SWAT : Descriptif_swap_de_taux.nominal_recu</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="18"/>
							<xs:fractionDigits value="5"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="nominalPay">
					<xs:annotation>
						<xs:documentation>SWAP nominal leg to pay</xs:documentation>
						<xs:documentation>SWAT : Descriptif_swap_de_taux.nominal_verse</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="18"/>
							<xs:fractionDigits value="5"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="circulation">
					<xs:annotation>
						<xs:documentation>Number of shares in circulation</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.nombre_titre</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="20"/>
							<xs:fractionDigits value="7"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="quota">
					<xs:annotation>
						<xs:documentation>Instrument lotsize</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.quotite_nego</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="10"/>
						</xs:restriction>
					</xs:simpleType>
				</xs:element>
				<xs:element name="votingRight">
					<xs:annotation>
						<xs:documentation>Number of voting rights</xs:documentation>
						<xs:documentation>VMOB : Descriptif_valeur.nombre_droits_vote</xs:documentation>
					</xs:annotation>
					<xs:simpleType>
						<xs:restriction base="xs:decimal">
							<xs:totalDigits value="18"/>
